datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" // Bisa ganti ke PostgreSQL/MySQL jika perlu
}

generator client {
  provider = "prisma-client-js"
}

model Absen {
  id        Int      @id @default(autoincrement())
  nama      String
  utusan    String
  pelatihan String
  tanggal   DateTime @default(now())
}
